{% extends 'base.html.twig' %}

{% block body %}
    {# {% include 'sidebar.html.twig' %} #}

    <div class="align-vertical">
        <div class="row column large-9 small-up-1 medium-up-2 align-vertical-up grid-list">
            {% for project in projects %}
                <div class="column project-panel">
                    <a href="{{ project.github }}" target="_blank" class="thumbnail-link">
                        <img class="thumbnail" src="{{ project.thumbnail }}">
                    </a>
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
